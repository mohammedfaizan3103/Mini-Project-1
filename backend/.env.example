# Backend Environment Variables - Example/Template
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration
MONGO_URI=mongodb://localhost:27017/your_database_name
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/database_name

# Session Configuration
SESSION_SECRET=your_super_secret_session_key_here

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173
# For production: https://your-frontend-domain.com

# API URL (for internal API calls)
API_URL=http://localhost:5000
# For production: https://your-backend-api-domain.com